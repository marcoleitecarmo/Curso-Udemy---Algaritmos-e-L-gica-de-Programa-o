Algoritmo "testeCondicional"


Var
   hora : inteiro

Inicio

   escreva("Digite uam hora do dia: ")
   leia(hora)
   
   se hora < 12 entao
      escrevaL("Bom dia!")
   senao
       se hora < 18 entao
           escreva("Boa Tarde")
       senao
           escreval("Boa Noite!")
       fimse
   fimse


Fimalgoritmo