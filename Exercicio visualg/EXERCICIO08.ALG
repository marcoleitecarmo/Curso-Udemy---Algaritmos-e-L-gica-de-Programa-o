Algoritmo "Exercicio08"


Var

 dpercorrida,combustivel, consumo : real


Inicio

 escreva("Digite a distância percorrida: ")
 leia(dpercorrida)
 escreva("Digite o gasto combustivel: ")
 leia(combustivel)
 consumo <- dpercorrida / combustivel

 escreval("Consumo médio: ", consumo:6:3)


Fimalgoritmo